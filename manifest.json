{
  "manifest_version": 2,

  "name": "KANOPE",
  "description": "Disable history, switch between no proxy and Tor , blocks some common attacks and trackers and more !",
  "version": "1.0",
  "icons": { 
          "128": "128.png" },
  "background": { "scripts": ["background.js"]},
  "permissions": ["tabs", "browsingData","privacy","proxy","webRequest", "webRequestBlocking", "<all_urls>", "offroad"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "KANOPE"
  }
}
