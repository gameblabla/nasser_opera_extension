{
  "manifest_version": 2,

  "name": "NASSER",
  "description": "Disable history , connects to Tor , blocks trackers and more !",
  "version": "1.0",
  "icons": { 
          "128": "128.png" },
  "background": { "scripts": ["background.js"]},
  "permissions": ["tabs", "browsingData","privacy","proxy","webRequest", "history", "webRequestBlocking", "<all_urls>", "offroad", "cookies"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "NESSA",
	"default_popup": "popup.html"
  }
}
